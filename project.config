[project]
	description = "Homescreen is the first screen that appears when the device is started.  This the center hub to launch applications."
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group pdxostc-management
	owner = group user/Steve Mattison (smattiso)
[access "refs/for/refs/*"]
	read = group user/mmorale1
	push = group user/mmorale1
	label-Code-Review = -2..+2 group user/mmorale1
	label-Verified = -1..+1 group user/mmorale1
	rebase = group user/mmorale1
	removeReviewer = group user/mmorale1
	submit = group user/mmorale1
[access "refs/heads/*"]
	read = group user/mmorale1
	create = group user/mmorale1
	push = +force group user/mmorale1
	pushMerge = group user/mmorale1
[access "refs/tags/*"]
	push = +force group user/mmorale1
	pushTag = group user/mmorale1
	pushSignedTag = group user/mmorale1
